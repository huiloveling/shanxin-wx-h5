.nav {
    position: relative;
    height: 45px;
    line-height: 45px;
    top: 0;
    left: 0;
    right: 0;
    z-index: 10;
    background: #fff;
}

.nav .back {
    position: absolute;
    left: 20rpx;
    top: 50%;
    height: 48rpx;
    bottom: 0;
}

.nav .back image {
    display: block;
    width: 38rpx;
    height: 38rpx;
}

.nav-title {
    height: 45px;
    line-height: 45px;
    position: absolute;
    text-align: center;
    left: 0;
    right: 0;
    bottom: 0;
    font-size: 36rpx;
    color: #000;
}

.overflow {
    overflow: auto;
    width: 100%;
    box-sizing: border-box;
    background: #f5f5f5;
    padding: 30rpx 30rpx 50rpx 30rpx;
}

.title {
    font-size: 32rpx;
    color:#333;
    font-family: 'PingFang-SC-Medium';
    margin-bottom: 30rpx;
}
.view-list {
    background: #fff;
    padding: 30rpx;
}

.view-list-title {
    height: 88rpx;
    line-height: 88rpx;
    display: flex;
    border: 1px solid red;
    border-radius: 50rpx;
}
.view-list-title view {
    flex: 1;
    text-align: center;
    font-size: 32rpx;
    color:#333;
    font-family: 'PingFang-SC-Medium';
}
.view-list-title view:first-child {
    border-right: 1px solid #f5f5f5;
    box-sizing: border-box;
}

.view-list-item {
    display: flex;
    height: 120rpx;
    line-height: 120rpx;
    align-items: center;
    justify-content: center;
    border-bottom: 1px solid #f5f5f5;
}
.view-list-item view image {
    display: block;
    height: 60rpx;
    width: 300rpx;
}

.view-list-item view {
    flex: 1;
    font-size: 30rpx;
    color: #ccc;
    text-align: center;
    display: flex;
    align-items: center;
    justify-content: center;
}