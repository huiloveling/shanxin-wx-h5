.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:36rpx;
  letter-spacing:2px;
  color: #fff;
  background: #1AAC19;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-white{
  background: #1AAC19;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}

.login-view {
    height: 100vh;
    background: url('https://sxapi.wanjingyou.com.cn/static/images/login.png');
    background-size: 100% 100%;
    padding-top: 300rpx;
    box-sizing: border-box;
}

.login-icon image {
    display: block;
    width: 170rpx;
    height: 170rpx;
    margin: 0 auto;
}
.login-btn {
    margin-top: 50rpx;
}
.login-btn button {
    display: block;
    width: 622rpx;
    height: 88rpx;
    margin: 0 auto;
    background: #fff;
    color: #07c160;
    border-radius: 50rpx;
    border: 1px solid #07c160;
}

.login-xieyi {
    display: flex;
    color: #fff;
    height: 50rpx;
    line-height: 50rpx;
    align-items: center;
    justify-content: center;
    margin-top: 30rpx;
}

.noLogin {
    margin-top: 30rpx;
    text-align: center;
}

.noLogin text {
    display: block;
    font-size: 30rpx;
    margin: 0 auto;
    border: none;
    color: #fff;
    text-decoration: underline;
}