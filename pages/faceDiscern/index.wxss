.nav {
    position: relative;
    height: 45px;
    line-height: 45px;
    top: 0;
    left: 0;
    right: 0;
    z-index: 10;
    background: #fff;
}

.nav-title {
    height: 45px;
    line-height: 45px;
    position: absolute;
    text-align: center;
    left: 0;
    right: 0;
    bottom: 0;
    font-size: 36rpx;
    color: #000;
}

.overflow {
    overflow: auto;
    width: 100%;
    box-sizing: border-box;
}

.hidden {
    overflow: hidden;
    
}

/* 上传人脸照片 */
.uploadFaceImg {
    background: #F6F6F6;
}

.uploadFaceImg .title {
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}

.uploadFaceImg .view-img {
    width: 100%;
    box-sizing: border-box;
    padding: 0 25rpx;
}

.uploadFaceImg .view-img image {
    display: block;
    width: 690rpx;
    height: 440rpx;
    margin: 0 auto;
}

.uploadFaceImg .view-img .again {
    position: relative;
}


.uploadFaceImg .view-img .again .btn {
    position: absolute;
    width: 160rpx;
    height: 160rpx;
    top: 50%;
    left: 50%;
    margin: -80rpx 0 0 -80rpx;
}

.explain {
    box-sizing: border-box;
    padding: 0 50rpx;
}
.explain view {
    font-family: 'PingFang-SC-Medium';
    font-size: 35rpx;
    color: #333;
    height: 70rpx;
    line-height: 70rpx;
}

button {
    width: 85%;
    margin: 0 auto;
    position: absolute;
    bottom: 100rpx;
    left: 50%;
    transform: translateX(-50%);
    color: #fff;
    background: #ccc;
}

button::after {
    border: none;
}

.images image {
    display: block;
    width: 502rpx;
    height: 88rpx;
    margin: 0 auto;
    position: absolute;
    bottom: 100rpx;
    left: 50%;
    transform: translateX(-50%);
}