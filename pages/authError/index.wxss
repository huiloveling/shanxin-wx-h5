.nav {
    width: 100%;
    overflow: hidden;
    position: relative;
    top: 0;
    left: 0;
    z-index: 10;
}

.nav-title {
    width: 100%;
    height: 45px;
    line-height: 45px;
    text-align: center;
    position: absolute;
    bottom: 0;
    left: 0;
    z-index: 10;
    font-family: PingFang-SC-Medium;
    font-size: 36rpx;
    letter-spacing: 2px;
}

.nav .back {
    width: 22px;
    height: 22px;
    position: absolute;
    bottom: 0;
    left: 0;
    padding: 10px 15px;
}

.bg-white {
    background-color: #fff;
}

.bg-gray {
    width: 100%;
    box-sizing: border-box;
    padding: 0 65rpx;
}

.overflow {
    overflow: auto;
}

.hidden {
    overflow: hidden;
    position: relative;
    padding-top: 300rpx;
    box-sizing: border-box;
}

.big {
    width: 626rpx;
    height: 250rpx;
}

.big image {
    display: block;
    width: 100%;
    height: 100%;
}

.text {
    text-align: center;
    font-family: '微软雅黑';
    margin: 50rpx 0 200rpx 0;
}

.btn {
    width: 100%;
    height: 46px;
}

.btn image {
    display: block;
    width: 100%;
    height: 100%;
}